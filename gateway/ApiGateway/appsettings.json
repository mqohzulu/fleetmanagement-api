{
  "ReverseProxy": {
    "Routes": {
      "identity_route": {
        "ClusterId": "identity_cluster",
        "Match": {
          "Path": "/identity/{**catch-all}"
        }
      },
      "fleet_route": {
        "ClusterId": "fleet_cluster",
        "Match": {
          "Path": "/fleet/{**catch-all}"
        }
      },
      "operations_route": {
        "ClusterId": "operations_cluster",
        "Match": {
          "Path": "/operations/{**catch-all}"
        }
      },
      "maintenance_route": {
        "ClusterId": "maintenance_cluster",
        "Match": {
          "Path": "/maintenance/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "identity_cluster": {
        "Destinations": {
          "identity1": { "Address": "http://identity:80/" }
        }
      },
      "fleet_cluster": {
        "Destinations": {
          "fleet1": { "Address": "http://fleet:80/" }
        }
      },
      "operations_cluster": {
        "Destinations": {
          "ops1": { "Address": "http://operations:80/" }
        }
      },
      "maintenance_cluster": {
        "Destinations": {
          "maint1": { "Address": "http://maintenance:80/" }
        }
      }
    }
  }
}
